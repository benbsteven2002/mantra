<script>
const swymRegistry_lsName = "swymKeys";
const swym_currency = "{{ cart.currency.symbol }}"
function swymRegistryAddProduct(product, registry) {
  try{
    var giftProduct = document.createElement('li');
    giftProduct.classList.add("swym-registry-owner-grid-item");
    giftProduct.innerHTML = `
      <a href="${product.du}" class="{{ settings.swym_registry_custom_link_class }}">
            <figure id="swym-registry-product-${product.epi}">
              <img src="${product.iu}" alt="swym-registry-product">
          </figure>
        </a>
          <div class="swym-registry-owner-grid-item-detail swym-registry-owner-grid-item-container-${product.epi}">
            <h2 class="swym-registry-font-regular">${product.dt}</h2>
            <ul class="swym-registry-owner-grid-item-center">
              <li><span class="swym-registry-owner-grid-item-price swym-registry-font-large">${swym_currency}${product.pr}</span></li>
              <li class="swym-registry-owner-grid-item-askqty-container swym-registry-font-very-small">
                <span class="swym-registry-owner-grid-item-want">{{ settings.swym_registry_want_label }}</span>
                <span class="swym-registry-owner-grid-item-want-value"> ${product.askQuantity}</span>
              </li>
              <li class="swym-registry-owner-grid-item-boughtqty-container swym-registry-font-very-small">
                <span class="swym-registry-owner-grid-item-purchased">{{ settings.swym_registry_purchase_label }}</span>
                <span class="swym-registry-owner-grid-item-want-value"> ${product.boughtQuantity}</span>
              </li>
            </ul>
            <ul class="swym-registry-owner-grid-item-bottom">
              <li>
                <span class="swym-registry-owner-grid-item-bottom-reduce" onclick="swymRegistryChangeQty(this)">-</span>
                 <input type="number" value="${product.askQuantity}" class="swym-registry-owner-grid-item-bottom-qty" pattern="[0-9]*" aria-label="quantity">
                <span class="swym-registry-owner-grid-item-bottom-increase" onclick="swymRegistryChangeQty(this, 'increase')">+</span>
              </li>
              <li class="swym-registry-owner-update-btn-container">
                <button class="swym-registry-font-btn {{ settings.swym_registry_custom_btn_class }}" onclick="swymUpdateQtyValue(this, ${product.epi})" class="swym-registry-registrant-update-btn">
                <div class="swym-registry-btn-dot-collision" id="swym-registry-btn-dot-collision_${product.epi}" style="display: none">
                  <div class="swym-registry-snippet" data-title=".dot-collision">
                    <div class="swym-registry-stage">
                      <div class="swym-registry-dot-collision"></div>
                    </div>
                  </div>
                </div>
                <span class="swym-registry-update-qty-btn-txt" id="swym-registry-update-qty-btn-txt_${product.epi}">{{ settings.swym_registry_gifter_grid_update_qty_btn }}</span> 
                </button>
              </li>
            </ul>
             <input type="hidden" value=${product.epi} class="swym-registry-product-meta-epi">
              <input type="hidden" value=${product.empi} class="swym-registry-product-meta-empi">
              <input type="hidden" value=${product.du} class="swym-registry-product-meta-du">
          </div>`;
    
      var productGrid = document.querySelector(".swym-registry-owner-grid-items");
      productGrid.appendChild(giftProduct);
      if(giftProduct.querySelector(".swym-registry-owner-grid-item-bottom-qty")){
       giftProduct.querySelector(".swym-registry-owner-grid-item-bottom-qty").addEventListener("input", ( event ) => {
          let targetValue = event.target.value;
          if(targetValue < 0){
            event.target.value = 0;
          }
        });
      }
      if(product && product.inventoryPolicy == 'DENY' && product.inventoryQuantity != undefined && product.inventoryQuantity != null &&  product.inventoryQuantity <= 0 && product.inventoryTracked === true){
        var image = document.getElementById(`swym-registry-product-${product.epi}`)
        var imageTag = document.createElement('span');
        imageTag.classList.add('swym-registry-product-banner', 'swym-registry-font-small');
        imageTag.innerHTML=`{{ settings.swym_registry_sold_out_label }}`;
        image.appendChild(imageTag);
      } else if(product.boughtQuantity >= product.askQuantity){
        var image = document.getElementById(`swym-registry-product-${product.epi}`)
        var imageTag = document.createElement('span');
        imageTag.classList.add('swym-registry-product-banner', 'swym-registry-font-small');
        imageTag.innerHTML=`{{ settings.swym_registry_purchased_label }}`
        image.appendChild(imageTag)
    }
  } catch(error) {
      console.log("Error in showing registry pages", error);
  }    
}
async function swymRegistryOnloadHandler() {
     if(!Shopify.designMode) {
       if(document.querySelector(".swym-registry-owner-container").clientWidth<1024 && window.outerWidth!=document.querySelector(".swym-registry-owner-container").clientWidth){
         document.querySelector(".swym-registry-owner-banner-content").style.width="60%";
       }
       if(document.querySelector('.swym-registry-order-button-1')){
         document.querySelector('.swym-registry-order-button-1').addEventListener('click',() => {
            _LTracker.push({
              'eventInfo': 'Send thanks button clicked - Owner',
              'tag': 'event'
            });
            Shopify.analytics.publish("swym_registry_registrant_view",{"eventAction": "Send thanks button clicked","customerInfo":{
              "name"  : "{{ customer.name }}",
              "email" : "{{ customer.email }}",
              "id"    : "{{ customer.id }}"
            }});
         });
         }
      if(document.querySelector('.swym-registry-order-button')){
       document.querySelector('.swym-registry-order-button').addEventListener('click',() => {
          _LTracker.push({
            'eventInfo': 'Track order button clicked - Owner',
            'tag': 'event'
          });
          Shopify.analytics.publish("swym_registry_registrant_view",{"eventAction": "Track order button clicked","customerInfo":{
            "name"  : "{{ customer.name }}",
            "email" : "{{ customer.email }}",
            "id"    : "{{ customer.id }}"
          }});
       });
         }
      if(document.querySelector('#swym-registry-share-mail-link')){
       document.querySelector('#swym-registry-share-mail-link').addEventListener('click',() => {
          _LTracker.push({
            'eventInfo': 'Share by mail button clicked - Owner',
            'tag': 'event'
          });
          Shopify.analytics.publish("swym_registry_registrant_view",{"eventAction": "Share by mail button clicked","customerInfo":{
            "name"  : "{{ customer.name }}",
            "email" : "{{ customer.email }}",
            "id"    : "{{ customer.id }}"
          }});
        });
      }
       window.swymRegistryCustomer = {
        "customerId": "{{ customer.id }}",
        "email": "{{ customer.email }}",
        "firstName": "{{ customer.first_name }}",
        "lastName": "{{ customer.last_name }}",
        "phone": "{{ customer.phone }}",
        "acceptsMarketing": "{{ customer.accepts_marketing }}"
      }
        var swymRegistry_lsInit = JSON.parse(localStorage.getItem(swymRegistry_lsName));
        const swymRegistry_authData = await swymRegistry_refreshAuth(swymRegistryCustomer, swymRegistry_lsInit);
        const token = swymRegistry_authData.swym_registry.jwt.token;
        const queryString = window.location.search;
        const urlParams = new URLSearchParams(queryString);
        const registryId = urlParams.has('registryId') ? urlParams.get('registryId') : "";
        const customerEmail = "{% if customer %}{{ customer.email }}{% endif %}";
        window.swymgr = {};
        window.swymgr.pid = swymRegistry_authData.swym_registry.pid;
        window.swymgr.registryId = registryId;
        window.swymgr.addProductsLink = "{{ settings.swym_registry_add_products_link }}";
        
       swymRegistry_getRegistry_owner(registryId,customerEmail,token).then(response => response.json())
        .then(resp => {
          window.swymgr.registryDetails = resp;
          var productList = swymgr.registryDetails.associatedListedProducts;
          if(productList.length >= 0){
          var registryInfo = swymgr.registryDetails;
          for(let i =0; i<productList.length; i++){
            swymRegistryAddProduct(productList[i], registryInfo);
          } 

        var registrantView = document.querySelector("#swym-registry-registrant-view");
       
        registrantView.innerHTML = `
          <div class="swym-registry-registrant-grid-parent">
            <div class="swym-registry-registrant-grid-nav">
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small swym-registry-border-btm-active" onclick="swymRegistryFilterProducts('all',this,'owner')">
                {{ settings.swym_registry_all_option_label }}
              </button>
              <div class="swym-registry-font-regular">|</div>
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small" onclick="swymRegistryFilterProducts('stillneed',this,'owner')">{{ settings.swym_registry_still_need_option_label }}</button>
              <div class="swym-registry-font-regular">|</div>
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small" onclick="swymRegistryFilterProducts('fullfilled',this,'owner')">{{ settings.swym_registry_fulfilled_option_label }}</button>
              <div class="swym-registry-font-regular">|</div>
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small" onclick="swymRegistryFilterProducts('soldout',this,'owner')">{{ settings.swym_registry_sold_out_option_label }}</button>
            </div>
             <ul class="swym-registry-owner-grid-top">
              <li class="swym-registry-owner-grid-top-add-products">
            <h4 class="swym-registry-owner-grid-product-count swym-registry-font-small"><span class="swym-registry-owner-grid-product-count-number">${swymgr.registryDetails.associatedListedProducts.length}</span><span> {{ settings.swym_registry_products_label }}</span></h4>
              <button class="swym-registry-font-btn {{ settings.swym_registry_custom_btn_class }}" onclick="swymRegistryRedirectToCollection()">+ {{ settings.swym_registry_gifter_grid_add_products_btn }}</button>
            </li>
              
            </ul>
          </div>`;
          document.getElementById("swym-registry-owner-banner").classList.remove('swym-registry-hidden');
           if(!swymgr.registryDetails.coCreatorName || swymgr.registryDetails.coCreatorName.trim().length === 0){
              document.querySelector('.swym-registry-coregistrant-name-label').remove();
           }

          let expiryDate = swymDaysBetween(swymgr.registryDetails.expiryDate);
          swymCounterAnim(".swym-registry-owner-banner-bottom-days.swym-registry-desktop strong", 0, expiryDate, 1500);
          swymCounterAnim(".swym-registry-owner-banner-bottom-days.swym-registry-mobile strong", 0, expiryDate, 1500);
          document.getElementById("swym-registry-event-type-value").textContent = swymgr.registryDetails.occasion;
          document.getElementById("swym-registry-event-date-value").textContent = swymgr.registryDetails.expiryDate;
          document.getElementById("swym-registry-id-value").textContent = swymgr.registryDetails.Id;
          document.getElementById("swym-registry-registrant-name-value").textContent = swymgr.registryDetails.creatorName;
          document.getElementById("swym-registry-coregistrant-name-value").textContent = swymgr.registryDetails.coCreatorName;
          document.querySelector(".swym-registry-owner-banner-bottom-days strong").textContent = swymDaysBetween(swymgr.registryDetails.expiryDate);
          document.querySelector(".swym-registry-owner-banner-bottom-days strong").textContent = swymDaysBetween(swymgr.registryDetails.expiryDate);
          document.querySelector("#swym-registry-owner-gifts-added").textContent = swymgr.registryDetails.associatedListedProducts.length;
          document.querySelector("#swym-registry-owner-orders-placed").textContent = swymgr.registryDetails.associatedBoughtProducts? swymgr.registryDetails.associatedBoughtProducts.length : 0;
         
          let swymRegistry_fname = swymgr.registryDetails.address.firstName, swymRegistry_lname = swymgr.registryDetails.address.lastName, swymRegistry_initials;
          if(swymRegistry_fname && swymRegistry_lname) {
            swymRegistry_initials = swymRegistry_fname.charAt(0) + swymRegistry_lname.charAt(0);
          }
          else {
             swymRegistry_initials = swymgr.registryDetails.creatorName.charAt(0);
          }
          document.querySelector(".swym-registry-owner-banner-user").textContent = swymRegistry_initials;
          document.querySelector(".swym-registry-owner-banner-title").textContent = swymgr.registryDetails.registryName;
          document.querySelector("#swym-registry-share-registry-name").value = swymgr.registryDetails.registryName;
          document.querySelector("#swym-registry-share-registry-id").value = swymgr.registryId;
          document.querySelector(".swym-registry-owner-banner-message").textContent = swymgr.registryDetails.description;
          if(swymgr.registryDetails.description.trim().length===0){
              document.querySelector(".swym-registry-owner-banner-message").style.display='none';
          }
        }
      }).catch(error => {
          _LTracker.push({
            'ErrorLocation': 'Get registry details for owner API call - Owner Template',
            'ErrorInfo': {
              "error": {
                "msg": error.message,
                "Reference_stack": error.stack
              }
            },
           'MerchantInfo': {
            'pid': swymRegistry_authData.swym_registry.pid,
            'shop': Shopify.shop,
            'themeId': Shopify.theme.id,
            'themeName': Shopify.theme.name
          },
          'CustomerInfo': {
            'registryId': swymgr.registryId,
            'customerId': swymRegistry_authData.swym_registry.customerId,
            'customerEmail': swymRegistryCustomer.email,
            'userAgent': navigator.userAgent 
          }
          }); 
      });
     }
}   
swymRegistryAddOnloadEvent(swymRegistryOnloadHandler);
</script>
