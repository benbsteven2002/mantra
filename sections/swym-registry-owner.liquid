<script>
    function swymRegistryCloseShareModal() {
        document.getElementById("swym-registry-copy-btn").textContent = "{{ settings.swym_registry_copy_button_label}}";
        document.querySelector(".swym-registry-share-modal-overlay").style.display = "none";
        document.querySelector(".swym-registry-share-modal-container").style.display = "none";
    }                                                                                                                                                 
  function swymRegistryCopyRegistryLink() {
        _LTracker.push({
          'eventInfo': 'Copy button clicked - Owner',
          'tag': 'event'
        });
        Shopify.analytics.publish("swym_registry_registrant_view",{"eventAction": "Copy button clicked","customerInfo":{
          "name"  : "{{ customer.name }}",
          "email" : "{{ customer.email }}",
          "id"    : "{{ customer.id }}"
        }});
        let copyText = document.getElementById("swym-registry-copy-text");
        let copyTextBtn = document.getElementById("swym-registry-copy-btn");
        copyText.select();
        copyText.setSelectionRange(0, 99999); // For mobile devices
        navigator.clipboard.writeText(copyText.value);
        copyTextBtn.textContent = "{{ settings.swym_registry_copied_button_label }}"
    }

    if (document.querySelector("#swym-registry-share-modal-close")) {
        document.querySelector("#swym-registry-share-modal-close").addEventListener("click", function (e) {
            swymRegistryHideShareOverlay();
            swymRegistryHideShareModal();
        });
    }                                                                                                                                                  // 
function swymRegistryShowOrderModal() {
    document.querySelector(".swym-registry-order-modal-container").style.display = "block";
    document.querySelector(".swym-registry-order-modal-container").innerHTML =
        `<div class="swym-registry-order-modal">
  <button class="swym-order-modal-close" onclick="swymRegistryCloseOrderModal()" id="swym-registry-order-modal-close">&#x2715;</button>
  <div class="swym-registry-font-large">
      {{ settings.swym_registry_order_modal_list_title }}
    </div>
    <div class="swym-registry-order-font-xs-small">
      ${swymgr.registryDetails.registryName}   |  {{ settings.swym_registry_id_lable }} ${swymgr.registryId}
    </div>
  <br>
  <div class="swym-registry-order-modal-content">
  </div>
</div>`
    let boughtProducts = swymgr.registryDetails.associatedBoughtProducts;
    if (boughtProducts && boughtProducts.length) {
        let dict = {}
        boughtProducts.forEach((element) => {
            if (dict[element.orderId]) {
                dict[element.orderId].push(element);
            }
            else {
                dict[element.orderId] = [element];
            }
        })
        for (var key in dict) {
            var orderItem = document.createElement('div');
            orderItem.classList.add('swym-registry-order-card');
            orderItem.innerHTML = `
<span class="swym-registry-order-card-data">
  <table class="swym-registry-order-table">
    <tr class="swym-registry-order-row-1">
      <td class="swym-registry-font-order-id swym-registry-font-small">{{ settings.swym_registry_send_thanks_modal_order_label }} - <a class="{{ settings.swym_registry_custom_link_class }}" href="${dict[key][0].Order.orderStatusUrl}" target="_blank">${key}</a></td>
      <td><span class="swym-registry-font-order-id swym-registry-font-small">${dict[key].length}</span>
        <span class="swym-registry-order-font-xs-small"> {{ settings.swym_registry_send_thanks_modal_items_label }}</span></td>
    </tr>
    <tr>
      <td class="swym-registry-order-font-xs-small" colspan="2" id="swym-registry-td-order-modal-title-${key}">{{ settings.swym_registry_order_modal_purchased_title }} ${dict[key][0].Order.gifterName} On ${new Date(dict[key][0].Order.createdAt).toDateString()} </td>
    </tr>
    <tr>
      <td colspan="2"  style="padding-bottom:2%;">
        <a class="{{ settings.swym_registry_custom_link_class }} swym-registry-order-card-msg-accordion" id="swym-registry-order-card-msg-accordion-${key}" onclick="swymRegistryopenClose(this)"><span class="swym-registry-order-font-xs-small" style="text-decoration: underline;">{{settings.swym_registry_order_modal_message_title}} ${dict[key][0].Order.gifterName} </span> </a> 
        <div class="swym-registry-order-card-msg-panel swym-registry-order-font-xs-small" id="swym-registry-order-card-msg-panel-${key}">
          ${dict[key][0].Order.note}
        </div>
      </td>               
    </tr>
    <tr>
      <td colspan="2" class="swym-registry-order-font-xs-small">
        <a class="{{ settings.swym_registry_custom_link_class }} swym-registry-order-button-1 swym-registry-font-small" id="swym-registry-order-button-1-${key}" href="mailto:${dict[key][0].Order.gifterEmail}?subject=${swymgr.registryDetails.registryName.replace(" ", "%20")} - ${encodeURIComponent(swymRegistryCapitalize(swymgr.registryDetails.occasion))}%20-%20Thanks" target="_blank" rel="noopener noreferrer">{{ settings.swym_registry_send_thanks_modal_send_thanks_label }}</a><a class="swym-registry-order-button swym-registry-font-small" id="" href="${dict[key][0].Order.orderStatusUrl}" target="_blank">{{ settings.swym_registry_send_thanks_modal_track_order_label }}</a>
      </td>
    </tr>
  </table>
</span>`
            document.querySelector(".swym-registry-order-modal-content").appendChild(orderItem);
            if(!dict[key][0].Order.gifterName){
              document.querySelector(`#swym-registry-td-order-modal-title-${key}`).style.display='none';
              document.querySelector(`#swym-registry-order-card-msg-accordion-${key}`).style.display='none';
            }
            else if(!dict[key][0].Order.note){
              document.querySelector(`#swym-registry-order-card-msg-accordion-${key}`).style.display='none';
            }
            if(!dict[key][0].Order.gifterEmail){
              document.querySelector(`#swym-registry-order-button-1-${key}`).href =`mailto:?subject=${swymgr.registryDetails.registryName.replace(" ", "%20")} - ${encodeURIComponent(swymRegistryCapitalize(swymgr.registryDetails.occasion))}%20-%20Thanks` ;
            }
        }
    }
    else {
        var emptyState = document.createElement('div');
        emptyState.classList.add('swym-registry-order-none');
        emptyState.innerHTML = `<p class='{{ settings.swym_registry_custom_para_class }} swym-registry-order-font-regular'>{{ settings.swym_registry_no_orders_yet_label }}</p>
 <p class="{{ settings.swym_registry_custom_para_class }} swym-registry-order-font-regular">{{ settings.swym_registry_share_registry_label }}</p>`
        document.querySelector(".swym-registry-order-modal-content").appendChild(emptyState);
    }
}
</script>
