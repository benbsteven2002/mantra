<script>

async function swymRegistryOnloadHandler() {
  if(!Shopify.designMode){
    try {
        const swymRegistry_lsName = "swymKeys";
        var swymRegistry_lsInit = JSON.parse(localStorage.getItem(swymRegistry_lsName));
        const swymRegistry_authData = await swymRegistry_refreshPid(swymRegistry_lsInit);
        const pid = swymRegistry_authData.swym_registry.pid;
        window.swymgr = {};
        window.swymgr.pid = pid;
        let queryString = window.location.search;
        let urlParams = new URLSearchParams(queryString);
        var searchTextUrl = urlParams.has("searchText") ? urlParams.get("searchText") : "";
        var all_registries;
        var searchBtn = document.getElementById("swym-registry-search-btn");
        var searchForm = document.querySelector(".swym-registry-search-registry");
        var viewMoreBtn = document.getElementById("swym-registry-view-more-btn");
        var noResult = document.querySelector(".swym-registry-no-results");
        window.swymGrSearch = {};
        window.swymGrSearch.pageItems = 10;
        window.swymGrSearch.renderedRegistries = 0;
  
        if (searchTextUrl) {
          try {
            swymSearchRegistryByString(searchTextUrl);
            document.getElementById("searchText").value = searchTextUrl;
          } catch (error) {
            console.log("Could not search from Url Params", error);
          }
        }
  
        viewMoreBtn.onclick = async function (e) {
          try {
            e.preventDefault();
            if(swymGrSearch.renderedRegistries === swymGrSearch.totalRegistries) {
              window.scrollTo({top: 0, behavior: 'smooth'});
              if(!swymGrCustomer.email){
                Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "ScrollToTop button clicked"});
              }
              else{
                Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "ScrollToTop button clicked","customerInfo":{
                  "name"  : "{{ customer.name }}",
                  "email" : "{{ customer.email }}",
                  "id"    : "{{ customer.id }}"
                }});
              }
            }
            else {
              if(!swymGrCustomer.email){
                Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "LoadMore button clicked"});
              }
              else{
                Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "LoadMore button clicked","customerInfo":{
                  "name"  : "{{ customer.name }}",
                  "email" : "{{ customer.email }}",
                  "id"    : "{{ customer.id }}"
                }});
              }
              swymAddRegistriesToListOnDemand();
            }
          } catch (error) {
            console.log("Error in showing more Registries", error)
          }
        }
  
        searchForm.addEventListener("submit", function (event) {
          event.preventDefault();
          if(searchBtn.style.pointerEvents !== "none"){
            try {
              
              if(!swymGrCustomer.email){
                Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "Search button clicked"});
              }
              else{
                Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "Search button clicked","customerInfo":{
                  "name"  : "{{ customer.name }}",
                  "email" : "{{ customer.email }}",
                  "id"    : "{{ customer.id }}"
                }});
              }
              searchBtn.style.pointerEvents = "none";
              var swymButtonLoader = document.querySelector("#swym-registry-btn-dot-collision-search");
              var btnText = document.querySelector("#swym-registry-search-text");
              window.swymGrSearch.renderedRegistries = 0;
              var searchText = document.getElementById('searchText').value;
              document.querySelector(".swym-registry-find-lists").innerHTML = "";
              document.querySelector(".swym-registry-find-lists-container").style.display = "none";
              document.querySelector(".swym-registry-view-more .swym-registry-load-more").innerHTML = "{{ settings.swym_registry_loadmore_button_text }}";
              swymButtonLoader.style.display = 'block';
              btnText.style.visibility = 'hidden';
              swymSearchRegistryByString(searchText);
              window.swymGrSearch.searchString = searchText;
            } catch (error) {
              console.log("Error on search press", error);
            }
          }
        });
  
        async function swymSearchRegistryByString(searchString) {
          try {
            var findListsContainer = document.querySelector(".swym-registry-find-lists-container");
            swymSearchRegistry(searchString, 0, window.swymGrSearch.pageItems).then((response) => response.json()).then((all_registries) => {
              window.swymGrSearch.currentPage = 0;
              window.swymGrSearch.searchString = searchString;
              window.swymGrSearch.totalRegistries = all_registries.totalResultsCount;
              
              if (swymGrSearch.totalRegistries) {
                swymAddRegistriesToList(all_registries.results);
                noResult.style.display = "none";
                document.querySelector(".swym-registry-count-container").style.display = 'flex';
                findListsContainer.style.display = "block";
                document.querySelector(".swym-registry-find-lists-header").style.display = "flex";
              }
              else {
                findListsContainer.style.display = "block";
                document.querySelector(".swym-registry-find-lists-header").style.display = "none";
                noResult.style.display = "flex";
                document.querySelector(".swym-registry-count-container").style.display = 'none';
                document.querySelector("#swym-registry-view-more-btn").style.display = "none";
                document.querySelector(".swym-registry-no-results div > span > span").innerText = searchString;
                searchBtn.style.pointerEvents = "auto";
              }
              var swymButtonLoader = document.querySelector("#swym-registry-btn-dot-collision-search");
              var btnText = document.querySelector("#swym-registry-search-text");
              swymButtonLoader.style.display = "none";
              btnText.style.visibility = 'initial';
            });
          } catch (error) {
            _LTracker.push({
                'ErrorLocation': 'Search Registry using searchString - API call',
                'ErrorInfo': {
                  "error": {
                    "msg": error.message,
                    "Reference_stack": error.stack
                  }
                },
               'MerchantInfo': {
                'pid': swymRegistry_authData.swym_registry.pid,
                'shop': Shopify.shop,
                'themeId': Shopify.theme.id,
                'themeName': Shopify.theme.name
              },
              'CustomerInfo': {
                'customerId': swymRegistry_authData.swym_registry.customerId,
                'customerEmail': swymGrCustomer.email,
                 'userAgent': navigator.userAgent 
              }
              });
            console.log("Error in Searching for registry", error);
          }
        }
        
        async function swymAddRegistriesToList(allRegistries) {
          try {
            window.swymGrSearch.renderedRegistries += allRegistries.length;
            if((window.swymGrSearch.totalRegistries <= window.swymGrSearch.pageItems)){
              document.querySelector("#swym-registry-view-more-btn").style.display = "none";
            }
            else if ((window.swymGrSearch.totalRegistries > window.swymGrSearch.renderedRegistries)) {
              viewMoreBtn.style.display = "flex";
            }
            else {
              document.querySelector(".swym-registry-view-more .swym-registry-load-more").innerHTML = "{{ settings.swym_registry_scrolltotop_button_text }}";
            }
             var renderedRegistries = window.swymGrSearch.renderedRegistries;
              var totalRegistries = window.swymGrSearch.totalRegistries;
              var registryFilter = document.querySelector(".swym-registry-find-lists-header");
              var currentCount = document.querySelector(".swym-registry-lists-current-count");
              var totalCount = document.querySelectorAll(".swym-registry-lists-total-count");
              currentCount.innerText = window.swymGrSearch.renderedRegistries;
  
              totalCount.forEach(function(item, index) {
               item.innerText = window.swymGrSearch.totalRegistries;
              });
  
            allRegistries.forEach((registry) => {
              let splitNames = registry.creatorName.split(' ');
              let fname, lname, initials
              if (splitNames.length >= 2) {
                fname = splitNames[0] || 'J'
                lname = splitNames[1] || 'D'
                initials = fname.charAt(0) + lname.charAt(0);
              }
              else {
                initials = registry.creatorName.charAt(0);
              }
              var registry_list = document.querySelector(".swym-registry-find-lists");
  
              var registry_list_li = document.createElement('li');
              registry_list_li.setAttribute("id", `registryid_${registry.Id}`);              
              var customSeparator = "{{ settings.swym_registry_custom_seperator }}"; // store separator setting;
  
              var showSeparator = registry.coCreatorName === null || registry.coCreatorName.match(/^ *$/) === null;
              let coregistrantLabel = "{{ settings.swym_registry_coregistrant_name_label }}";
              let isShelterName=(coregistrantLabel == "Co-registrant Name" ? false : true);
              registry_list_li.innerHTML += `
                  <div class="swym-registry-find-list-circle-parent swym-registry-find-lists">
                    <div class="swym-registry-find-list-circle">
                      <p class="swym-registry-font-very-large swym-registry-page-content {{ settings.swym_registry_custom_para_class }}">${initials}</p>
                    </div>
                  </div>
                  <div class="swym-registry-find-list-details-parent">
                    <div class="swym-registry-list-detail-title">
                      <span class="swym-registry-font-large swym-registry-page-content">${registry.creatorName}</span>
                      <span class="swym-registry-list-details-separator swym-registry-font-large swym-registry-page-content">${showSeparator ? customSeparator : "&nbsp;" }</span>
                      <span class="swym-registry-font-large swym-registry-page-content">${isShelterName && showSeparator ? `${coregistrantLabel} : `:""}${registry.coCreatorName}</span>
                    </div>
                    <div class="swym-registry-find-list-details">
                      <p class="swym-registry-font-small swym-registry-page-content {{ settings.swym_registry_custom_para_class }}">{{ settings.swym_registry_reg_id_label }} : <span><strong>${registry.Id}</strong></span></p>
                      <p class="swym-registry-font-small swym-registry-page-content {{ settings.swym_registry_custom_para_class }}">{{ settings.swym_registry_reg_name_label }} : <span><strong>${registry.registryName}</strong></span></p>
                    </div>
                    <p class="swym-registry-registry-description-title swym-registry-font-small {{ settings.swym_registry_custom_para_class }}">${registry.description ? "Registry Description" : ""}</p>
                    <p class="swym-registry-registry-description swym-registry-font-small-no-case {{ settings.swym_registry_custom_para_class }}">${registry.description ? registry.description : ""}</p>
                  </div>
                  <div class="swym-registry-list-column-detail swym-registry-list-detail-tab">
                    <span class="swym-registry-font-regular swym-registry-page-content"><span class="swym-registry-font-small swym-registry-list-detail-mobile">{{ settings.swym_registry_event_type_label }}&nbsp;:&nbsp;</span><span class="swym-registry-event-type-truncate"> ${registry.occasion}</span></span>
                    <span class="swym-registry-font-regular swym-registry-page-content"><span class="swym-registry-font-small swym-registry-list-detail-mobile">{{ settings.swym_registry_event_on_label }}&nbsp;</span> ${registry.expiryDate}</span>
                    <span class="swym-registry-font-regular swym-registry-page-content"><span class="swym-registry-font-small swym-registry-list-detail-mobile">{{ settings.swym_registry_event_from_label }}&nbsp;</span> ${registry.location}</span>
                    <button class="swym-registry-font-small-light swym-registry-btn-secondary swym-registry-list-detail-mobile {{ settings.swym_registry_custom_btn_class }}" id='swym-registry-view-registry-1-${registry.Id}'>{{ settings.swym_registry_view_registry_button_label }}</button>
                  </div>
                  <div class="swym-registry-list-data-mobile">
                    <span class="swym-registry-font-small swym-registry-page-content"><span class="swym-registry-font-small swym-registry-list-detail-mobile">{{ settings.swym_registry_event_type_label }}&nbsp;:&nbsp;</span><span class="swym-registry-event-type-truncate"> ${registry.occasion}</span></span>
                    <div class="swym-registry-list-additional-detail">
                      <span class="swym-registry-font-small swym-registry-page-content"><span class="swym-registry-font-small swym-registry-list-detail-mobile">{{ settings.swym_registry_event_on_label }}&nbsp;</span><b>${registry.expiryDate}</b></span>
                      <span class="swym-registry-font-small swym-registry-page-content">&nbsp;|&nbsp;</span>
                      <span class="swym-registry-font-small swym-registry-page-content"><span class="swym-registry-font-small swym-registry-list-detail-mobile">{{ settings.swym_registry_event_from_label }}&nbsp;</span><b>${registry.location}</b></span>
                    </div>
                    <div class="swym-registry-list-btn-container"> 
                      <button class="swym-registry-btn-secondary {{ settings.swym_registry_custom_btn_class }}" id='swym-registry-view-registry-2-${registry.Id}'>{{ settings.swym_registry_view_registry_button_label }}</button>
                    </div>
                  </div>
              `;
              async function swymRegistryHandleViewRegistry(e) {
                e.preventDefault();
                viewRegistryBtn = document.querySelector(".swym-registry-view-arrow");
                computedStyleViewRegistryBtn = getComputedStyle(viewRegistryBtn);
                if((viewRegistryBtn && computedStyleViewRegistryBtn && computedStyleViewRegistryBtn.display!=='none')||(e.target.classList.contains("swym-registry-btn-secondary"))){
                  if(!swymGrCustomer.email){
                    Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "View Registry button clicked"});
                  }
                  else{
                    Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "View Registry button clicked","customerInfo":{
                      "name"  : "{{ customer.name }}",
                      "email" : "{{ customer.email }}",
                      "id"    : "{{ customer.id }}"
                    }});
                  }
                  if (registry.settings && registry.settings.isPasswordProtected) {
                    swymGrPasswordModal();
                    var passwordBtn = document.querySelector(".swym-registry-password-modal-container .swym-registry-password-submit");
                    var popupInitials = document.querySelector(".swym-registry-password-modal-content .swym-registry-find-list-circle p");
                    var popupRegistryCreatorName = document.querySelector(".swym-registry-password-modal-content .swym-registry-find-list-creator-name");
                    var popupRegistryCoCreatorName = document.querySelector(".swym-registry-password-modal-content .swym-registry-find-list-co-creator-name");
                    var popupTitleSeparator = document.querySelector(".swym-registry-password-modal-content .swym-registry-find-list-separator");
                    var popupRegistryName = document.querySelector(".swym-registry-password-modal-content .swym-registry-find-list-registry-name");
                    popupInitials.innerHTML = initials;
                    popupRegistryCreatorName.innerHTML = registry.creatorName;
                    popupRegistryCoCreatorName.innerHTML = registry.coCreatorName;
                    popupTitleSeparator.innerHTML = ((registry.coCreatorName === null || registry.coCreatorName.match(/^ *$/) === null) ? " {{ settings.swym_registry_custom_seperator }} ":"");
                    popupRegistryName.innerHTML = registry.registryName + " Registry";
                    passwordBtn.onclick = async function (e) {
                      e.preventDefault();
                      if(!swymGrCustomer.email){
                        Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "Password Modal Submit button clicked"});
                      }
                      else{
                        Shopify.analytics.publish("swym_registry_find_view",{"eventAction": "Password Modal Submit button clicked","customerInfo":{
                          "name"  : "{{ customer.name }}",
                          "email" : "{{ customer.email }}",
                          "id"    : "{{ customer.id }}"
                        }});
                      }
                      viewBtn.style.pointerEvents = "auto";
                      var passwordValue = document.querySelector(".swym-registry-password-modal-container .swym-registry-field-input").value;
                      var payload = {
                        "password": `${passwordValue}`,
                        "registryId": `${registry.Id}`
                      }
                      const swymButtonLoader = document.querySelector('#swym-registry-btn-dot-collision-password');
                      const btnText = document.querySelector("#swym-registry-password-button-label");
                      try {
                        swymButtonLoader.style.display = 'block';
                        btnText.style.visibility = 'hidden';
                        generateAuthToken(payload).then((response) => (response.text())).then((grToken) => {
                          console.log(grToken);
                          if(grToken.includes("Invalid")){
                            swymButtonLoader.style.display = 'none';
                            btnText.style.visibility = 'initial';
                            swymGrPasswordMessage(`{{ settings.swym_registry_incorrect_password_label }}`);
                          }
                          else{
                            swymButtonLoader.style.display = 'none';
                            btnText.style.visibility = 'initial';
                            window.location = `/pages/swym-registry-gifter?registryId=${registry.Id}&shareToken=${encodeURIComponent(grToken)}`;
                          }
                        });
                      } catch (error) {
                        _LTracker.push({
                          'ErrorLocation': 'Generate Share Auth Token - API call',
                          'ErrorInfo': {
                            "error": {
                              "msg": error.message,
                              "Reference_stack": error.stack
                            }
                          },
                         'MerchantInfo': {
                            'pid': swymRegistry_authData.swym_registry.pid,
                            'shop': Shopify.shop,
                            'themeId': Shopify.theme.id,
                            'themeName': Shopify.theme.name
                          },
                          'CustomerInfo': {
                            'customerId': swymRegistry_authData.swym_registry.customerId,
                            'customerEmail': swymGrCustomer.email,
                             'userAgent': navigator.userAgent 
                          }
                        });
                        console.log("Generate Share Auth Token - API call: ",error);
                      }
                    }
                  }
                  else {
                    window.location = `/pages/swym-registry-gifter?registryId=${registry.Id}`;
                  }
                }
              }
              var viewBtn = document.createElement('span');
              viewBtn.setAttribute("class", `swym-registry-view-arrow`);
              viewBtn.innerText = ``;
              viewBtn.style.pointerEvents = "auto";
              registry_list_li.onclick = swymRegistryHandleViewRegistry;
              registry_list_li.querySelector(`#swym-registry-view-registry-1-${registry.Id}`).onclick = swymRegistryHandleViewRegistry;
              registry_list_li.querySelector(`#swym-registry-view-registry-2-${registry.Id}`).onclick = swymRegistryHandleViewRegistry;
              registry_list.appendChild(registry_list_li);
              document.querySelector(`#registryid_${registry.Id} .swym-registry-list-column-detail`).appendChild(viewBtn);
             
           viewBtn.innerHTML += `<svg xmlns="http://www.w3.org/2000/svg" width="12" height="19" viewBox="0 0 12 19" fill="none">
                                 <path d="M1 18L10 9.5L1 1" stroke={{ settings.swym_registry_page_content_color }} stroke-width="2"/>
                                </svg>`;
            });
            searchBtn.style.pointerEvents = "auto";
          }
          catch (error) {
            console.error("Error in showing list searched registries: ", error, all_registries);
          }
        }
  
        async function swymAddRegistriesToListOnDemand() {
  
          if (window.swymGrSearch.totalRegistries > window.swymGrSearch.renderedRegistries) {
            try {
              window.swymGrSearch.currentPage += 1;
                var swymButtonLoader = document.querySelector(".swym-registry-view-more .swym-registry-btn-dot-collision");
                var btnText = document.querySelector(".swym-registry-view-more .swym-registry-load-more");
                swymButtonLoader.style.display = 'block';
                btnText.style.visibility = 'hidden';
                swymSearchRegistry(window.swymGrSearch.searchString, window.swymGrSearch.currentPage, window.swymGrSearch.pageItems).then(res => res.json()).then(res => {
                  var all_registries = res;
                  swymAddRegistriesToList(all_registries.results);
                  swymButtonLoader.style.display = 'none';
                  btnText.style.visibility = 'visible';
                })
              
            } catch (error) {
              console.log("Error on adding more registries", error);
            }
          }
        }
      }
      catch (error) {
        console.log("Error in executing any page function", error);
      }
  }
  else{
    document.querySelector("#swym-registry-search-btn").addEventListener('click',(e)=>{
      e.preventDefault();
    });
    document.querySelector("#swym-registry-view-registry-design").onclick = async function (e) {
      e.preventDefault();
      viewRegistryBtn = document.querySelector(".swym-registry-view-arrow");
      computedStyleViewRegistryBtn = getComputedStyle(viewRegistryBtn);
      if((viewRegistryBtn && computedStyleViewRegistryBtn && computedStyleViewRegistryBtn.display!=='none')||(e.target.classList.contains("swym-registry-btn-secondary"))){
        swymGrPasswordModal();
      }
    }
    document.querySelector("#swym-registry-view-registry-design-1").onclick = async function (e) {
      e.preventDefault();
      swymGrPasswordModal();
    }
    document.querySelector("#swym-registry-view-registry-design-2").onclick = async function (e) {
      e.preventDefault();
      swymGrPasswordModal();
    }
  }
}
swymRegistryAddOnloadEvent(swymRegistryOnloadHandler)
</script>
