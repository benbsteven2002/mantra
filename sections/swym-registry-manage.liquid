  .swym-registry-manage-page-header .swym-registry-manage-page-header-title-parent .swym-registry-manage-page-header-info-divider {
    font-weight: bold;
  }

  /* Grid List CSS Here */
  .swym-registry-wrapper .swym-registry-manage-page-grid-section {
    padding: 1.5rem 0rem;
    margin: 10px 0;
    color: {{ settings.swym_registry_page_content_color }}
  }
  
