    display: flex;
    align-items: center;
    padding: 36px 0px;
    margin: 2rem 0;
    border: 1px solid {{ settings.swym_registry_page_content_color | color_modify: 'alpha', 0.15 }};
    border-radius: 1rem;
    position:relative;
  }

  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-circle-parent {
    text-align: center;
    width: 20%;
  }
  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-circle {
    display: inline-block;
    width: 8vw;
    height: 8vw;
    border-radius: 50%;
    align-self: center;
    position: relative;
    max-width: 150px;
    max-height: 150px;
    min-width: 60px;
    min-height: 60px;
    border: 1px solid {{ settings.swym_registry_page_content_color | color_modify: 'alpha', 0.7 }};
    align-self: flex-start;
  }
  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-circle p {
    margin: 0;
    text-align: center;
    position: absolute;
    top: 50%;
    transform: translate(-50%, -50%);
    display: inline-block;
  }
  
  .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-description-title {
    margin-bottom: 0.5rem;
    margin-top: 1rem;
  }

  .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-description {
    padding: 0.5rem;
  }
  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-registry-description:before, 
  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-registry-description:after {
    content: '"';
    font-size: 3rem;
    font-style: italic;
    vertical-align: text-top;
    left: -0.5rem;
    position: relative;
  }

  .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-info-parent {
    width: 80%;
  }
  .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-info {
    display: flex;
    justify-content: flex-start;
    width: 100%;
    flex-wrap: wrap;
    position: relative;
    padding: 0;
    margin: 0;
  }
  .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-info-list {
    padding-right: 1.5rem;
    margin: 0;
  }

  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-registry-btn-parent {
    display: flex;
    justify-content: flex-start;
    width: 100%;
    flex-wrap: wrap;
  }
  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-registry-btn {
    border-radius: 0.5rem;
    border: 1.5px solid {{ settings.swym_registry_page_content_color | color_modify: 'alpha', 0.4 }};
    padding: 0.3rem 2rem;
    background-color: {{ settings.swym_registry_page_content_background_color }};
    color: {{ settings.swym_registry_page_content_color }};
    cursor: pointer;
    margin: 1rem 0 0 0;
    font-size: calc(14px + ({{ settings.swym_registry_font_regular }} - 14) * ((100vw - 320px) / (1440 - 320)));
    transition: 0.24s ease;
    font-family: inherit;
  }
  .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-registry-btn:hover {
    transform: scale(1.06,1.06);
  }

  .swym-registry-wrapper .swym-registry-manage-page-card-parent {
    display: flex;
    flex-direction: column;
    gap: 2rem;
    padding: 3rem;
    align-items: center;
  }

  .swym-registry-wrapper #swym-registry-manage-page-create-btn {
    border-radius: 0.5rem;
    border: none;
    padding: 1rem 6rem;
    color: {{ settings.swym_registry_btn_text_color}};
    background: {{settings.swym_registry_btn_background_color }};
    cursor: pointer;
    font-size: calc(14px + ({{ settings.swym_registry_font_btn }} - 14) * ((100vw - 320px) / (1440 - 320)));
    text-align: center;
    transition: 0.24s ease;
  }
  .swym-registry-wrapper #swym-registry-manage-page-create-btn:hover {
    transform: scale(1.06,1.06);
  }
  .swym-registry-share-modal-overlay {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.7);
    overflow: auto;
    animation-name: animateopacity;
    animation-duration: 0.5s;
    z-index: 9999;
}
  .swym-registry-share-modal-container {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99999;
}

.swym-registry-share-modal-container .swym-registry-share-modal {
    position: relative;
    margin: 0 auto;
    background-color: {{ settings.swym_registry_page_content_background_color }};
    width: 45%;
    max-width: 1440px;
    height: auto;
    padding: calc(20px + (70 - 20) * ((100vw - 320px) / (1564 - 320)));
    animation: drop-in 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    transform: translate(0%, -25%);
    top: 25%;
}

.swym-registry-share-modal-container .swym-modal-close {
    position: absolute;
    right: 20px;
    border: none;
    background: none;
    font-size: 18px;
    cursor: pointer;
    top: 12px;
    color: {{settings.swym_registry_page_content_color}};
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    margin: 0;
    padding: 0;
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li {
    list-style-type: none;
    font-family: {{   header_font.family }};
    font-weight: 500;
    font-size: calc(14px + ({{ settings.swym_registry_font_regular }} - 14) * ((100vw - 320px) / (1440 - 320)));
    line-height: 24px;
    color: {{   settings.swym_registry_page_content_color }};
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li:first-child {
    margin-bottom: 20px;
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li:nth-child(2) {
    font-family: {{ header_font.family }};
    font-style: normal;
    font-weight: 400;
    line-height: normal;
    text-align: center;
    color: {{ settings.swym_registry_page_content_color }};
    margin-bottom: 20px;
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li:nth-child(3) {
    margin-bottom: 40px;       
    color: {{  settings.swym_registry_page_content_color}};
    font-weight: 600;
    text-align: center;
    line-height: 24px;
}
.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li:nth-child(5) button {
    border: none;
    padding: 12px 19px;
    color: {{ settings.swym_registry_btn_text_color}};
    background: {{settings.swym_registry_btn_background_color }};
    font-family: {{ header_font.family }};
    font-style: normal;
    font-weight: 500;
    text-align: center;
    width: fit-content;
    cursor: pointer;
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li:nth-child(5) {
    width: 90%;
    display: flex;
    margin-top: 30px;
    justify-content: center;
}

.swym-registry-share-modal-container .swym-registry-share-modal .swym-registry-share-modal-content ul li:nth-child(5) input {
    width: 70%;
    outline: none;
    border: none;
    background: none;
    border: 1px solid {{ settings.swym_registry_page_content_color}};
    padding: 12px;
    margin: 0;
}
.swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-edit,
.swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-share {
  cursor: pointer;
  position:absolute;
  top: 20px;
}
.swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-edit{
  right: 65px;
}
.swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-share{
  right: 26px;
}
.swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-edit svg{
  fill: {{ settings.swym_registry_page_content_color }};
}
.swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-share svg{
  fill: {{ settings.swym_registry_page_content_color }};
}
.swym-registry-share-modal-container .swym-registry-share-modal-content #swym-registry-copy-text{
  color: {{ settings.swym_registry_page_content_color }};
  font-family: inherit;
  outline: none;
  box-shadow: none;
}
.swym-registry-share-modal-container .swym-registry-share-modal-content .swym-registry-envelop-icon{
  width: 20px;
}

.swym-registry-wrapper #swym-registry-manage-page-empty-section{
  color: {{settings.swym_registry_page_content_color}};
}
  
@media(max-width: 1040px) {
    .swym-registry-share-modal-container .swym-registry-share-modal-content ul li:first-child {
        display: flex;
        flex-direction: column;
    }
}

@media screen and (max-width: 850px) {
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-lists-registry-btn-parent{
      justify-content: center;  
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-lists-registry-info-parent {
      width: initial;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-lists-registry-info-parent h3 {
      text-align: center;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-lists-registry-description-title {
      text-align: center;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-lists-registry-description {
      text-align: center;
    }
    .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-registry-btn-parent {
      justify-content: center;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-info {
      justify-content: center;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-info-parent {
      width: 100%
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-edit, .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-share {
      top: 10px;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-edit {
      right: 50px;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-section .swym-registry-gifter-banner-share {
      right: 16px;  
    }
    .swym-registry-manage-page-grid-section .swym-registry-manage-page-grid-parent .swym-registry-manage-page-grid-lists-list{
      flex-direction: column;
    }
    .swym-registry-wrapper .swym-registry-manage-page-grid-lists-registry-description {
