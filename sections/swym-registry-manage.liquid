      <div class="swym-registry-manage-page-header-title-parent">
        <h1 class="{{ settings.swym_registry_custom_title_class }} swym-registry-manage-page-header-title swym-registry-font-title">
          {{ settings.swym_registry_manage_title_text }}
        </h1>
        <ul class="swym-registry-manage-page-header-info-parent">
          <li><span id="swym-registry-manage-page-header-info-total-registries" class="swym-registry-font-small">2 {{ settings.swym_registry_registries_label }}</span></li>
          <li class="swym-registry-manage-page-header-info-divider swym-registry-font-small">|</li>
          <li>
            <a class="{{ settings.swym_registry_custom_link_class }} swym-registry-font-small" href="/pages/swym-registry-form?mode=create">
              {{ settings.swym_registry_form_create_title_text }}
            </a>
          </li>
        </ul>
      </div>
    </section>
    <section class="swym-registry-manage-page-grid-section">
      <div class="swym-registry-manage-page-grid-parent">
