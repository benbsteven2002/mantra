    position: relative;
  }
  .swym-registry-wrapper .swym-registry-manage-page-header-title {
    text-align: center;
    margin: 0;
    padding: 4rem 0 0 0;
  }

  .swym-registry-manage-page-header .swym-registry-manage-page-header-title-parent .swym-registry-manage-page-header-info-parent {
    display: flex;
    gap: 2rem;
    list-style: none;
    justify-content: center;
    padding: 2rem 0 3rem 0;
    margin: 0 auto;
    width: 90%;
