@media(max-width: 790px) {
    .swym-registry-share-modal-container .swym-registry-share-modal {
        width: 80%;
    }
}
  
@media(max-width: 670px) {
    ul.swym-registry-manage-page-grid-lists-registry-info.swym-registry-font-small{
      margin-left: 2%;
    }
}

@media(max-width: 470px) {
  .swym-registry-share-modal-container .swym-registry-share-modal {
    width: 95%;
