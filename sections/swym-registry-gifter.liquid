        swymRegistryAddToShopifyCart(swymgr.checkoutProducts).then(addedToCart => {
          if(addedToCart) {
            // Add / modify the cart badges in gifter page
            swymRegistryAddCartBadge();
        
            swymRegistryShowToast();
          }
          else 
            console.log("Error in adding to Shopify Cart");
        })
      } else {
        // Open side cart as well
        const swymRegistryCartProduct = Object.keys(swymgr.checkoutProducts).length;
        document.getElementById("swym-registry-gifters-cart-count").innerHTML = `&nbsp;(${swymRegistryCartProduct})`;
        swymRegistryShowToast();
        openSwymRegistryGifterModal();
      }
    } catch(error) {
      console.log("Error in creating checkout", error);
    }
    if(!window.swymGrSettings || !window.swymGrSettings?.isShopifyCartEnabled) {
      if(Object.keys(swymgr.checkoutProducts).length===0){
        let checkoutBtn = document.querySelector(".swym-registry-gifters-checkout");
