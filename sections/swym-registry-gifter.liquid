        document.querySelector(".swym-registry-gifters-checkout").disabled=false;
        let swymGiftCartBtn = document.querySelector('.swym-registry-gifter-cart-btn-container');
        swymGiftCartBtn.classList.remove('swym-registry-gifters-btn-disabled');
      }
    }
