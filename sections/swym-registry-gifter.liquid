async function swymRegistryOnloadHandler() {
  if(!Shopify.designMode) {
        window.swymRegistryCustomer = {
          "customerId": "{{ customer.id }}",
          "email": "{{ customer.email }}",
          "firstName": "{{ customer.first_name }}",
          "lastName": "{{ customer.last_name }}",
          "phone": "{{ customer.phone }}",
          "acceptsMarketing": "{{ customer.accepts_marketing }}"
        }
        var swymRegistry_lsInit = JSON.parse(localStorage.getItem(swymRegistry_lsName));
        const swymRegistry_authData = await swymRegistry_refreshPid(swymRegistry_lsInit);
        const pid = swymRegistry_authData.swym_registry.pid;
        const queryString = window.location.search;
        const urlParams = new URLSearchParams(queryString);
        const registryId = urlParams.has('registryId') ? urlParams.get('registryId') : "";
        const customerEmail = "{% if customer %}{{ customer.email }}{% endif %}";
        let shareToken = urlParams.has('shareToken') ? urlParams.get('shareToken') : "";
        window.swymgr = {};
        window.swymgr.pid = pid;
        window.swymgr.registryId = registryId;
        swymRegistry_getRegistry_gifter(registryId,shareToken,swymgr.pid).then(response => response.json())
        .then(resp => {
          window.swymgr.registryDetails = resp;

          // Add the current registry details to LocalStorage Object
          const swym_registry_details = {
            registrant: {
              firstName: resp.creatorName && resp.creatorName.split(" ")[0] ? resp.creatorName.split(" ")[0] : "",
              lastName: resp.creatorName && resp.creatorName.split(" ")[1] ? resp.creatorName.split(" ")[1] : "",
              registryName: resp.registryName ? resp.registryName : "",
              registryId: resp.Id ? resp.Id : "",
              email: resp.email ? resp.email : ""
            },
            address: resp.address ? resp.address : {}
          };
          localStorage.setItem('swym_registry_details', JSON.stringify(swym_registry_details));

          var productList = swymgr.registryDetails.associatedListedProducts;
          if(productList.length >= 0){
          var registryInfo = swymgr.registryDetails;
          for(let i =0; i<productList.length; i++){
            if (productList[i].hasOwnProperty("onlineStoreUrl")) {
              swymRegistryAddProduct(productList[i], registryInfo);
            }
          } 
          
        const swymRegistryProductGridList = document.querySelector(".swym-registry-gifter-cart-btn-container"); 
        if(productList.length >= 4) {
            swymRegistryProductGridList.style.display="flex";
        }
        var registrantView = document.querySelector("#swym-registry-registrant-view");
        let isShopifyCartEnabled = window.swymGrSettings && window.swymGrSettings.isShopifyCartEnabled;
        
        registrantView.innerHTML = `
          <div class="swym-registry-registrant-grid-parent">
            <div class="swym-registry-registrant-grid-nav">
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small swym-registry-border-btm-active" onclick="swymRegistryFilterProducts('all',this,'gifter')">
                {{ settings.swym_registry_all_option_label }}
              </button>
              <div class="swym-registry-font-regular">|</div>
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small" onclick="swymRegistryFilterProducts('stillneed',this,'gifter')">{{ settings.swym_registry_still_need_option_label }}</button>
              <div class="swym-registry-font-regular">|</div>
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small" onclick="swymRegistryFilterProducts('fullfilled',this,'gifter')">{{ settings.swym_registry_fulfilled_option_label }}</button>
              <div class="swym-gr-font-regular">|</div>
              <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-border-btn-filters swym-registry-font-small" onclick="swymRegistryFilterProducts('soldout',this,'gifter')">{{ settings.swym_registry_sold_out_option_label }}</button>
            </div>
             <ul class="swym-registry-gifter-grid-top">
              <li class="swym-registry-gifter-grid-top-add-products">
              <h4 class="swym-registry-gifter-grid-product-count swym-registry-font-small"><span class="swym-registry-gifter-grid-product-count-number">${swymgr.registryDetails.associatedListedProducts.length}</span><span> {{ settings.swym_registry_products_label }}</span></h4>
              <div class="swym-registry-gifters-checkout-container swym-registry-gifters-btn-disabled">
              ${
                isShopifyCartEnabled ? 
                  `<div></div>` : 
                  `<button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-gifters-checkout swym-registry-font-btn" onclick="openSwymRegistryGifterModalHandler() ">
                    {{ settings.swym_registry_gifter_grid_checkout_btn }}
                    <span class="swym-registry-gifters-cart-count" id="swym-registry-gifters-cart-count"></span>
                  </button>  
                  <span class="swym-registry-gifters-tooltiptext">{{ settings.swym_registry_tooltip_label }}</span>`
                }
                
              </div>
              </li>
            </ul>
          </div>`;
  
          document.getElementById("swym-registry-gifter-banner").classList.remove('swym-registry-hidden');
          let expiryDate = swymDaysBetween(swymgr.registryDetails.expiryDate);
          swymCounterAnim(".swym-registry-gifter-banner-bottom-days strong", 0, expiryDate, 1500);

          document.getElementById("swym-registry-event-type-value").textContent = swymgr.registryDetails.occasion;
          document.getElementById("swym-registry-event-date-value").textContent = swymgr.registryDetails.expiryDate;
          document.getElementById("swym-registry-id-value").textContent = swymgr.registryDetails.Id;
          document.getElementById("swym-registry-registrant-name-value").textContent = swymgr.registryDetails.creatorName;
          document.getElementById("swym-registry-coregistrant-name-value").textContent = swymgr.registryDetails.coCreatorName;
          document.querySelector(".swym-registry-gifter-banner-bottom-days strong").textContent = swymDaysBetween(swymgr.registryDetails.expiryDate);

              
         let swymRegistryCreatorNameParts = swymgr.registryDetails.creatorName.split(" ");
          let swymRegistryCreatorFirstName = swymRegistryCreatorNameParts[0];
          let swymRegistryCreatorLastName = swymRegistryCreatorNameParts[1]; let swymRegistry_initials;
          
          if(swymRegistryCreatorFirstName && swymRegistryCreatorLastName) {
            swymRegistry_initials = swymRegistryCreatorFirstName.charAt(0) + swymRegistryCreatorLastName.charAt(0);
          }
          else {
             swymRegistry_initials = swymgr.registryDetails.creatorName.charAt(0);
          }

          if(!swymgr.registryDetails.coCreatorName || swymgr.registryDetails.coCreatorName.trim().length === 0){
            document.querySelector('.swym-registry-gifter-co-registrant-name-label').remove();
          }
          document.querySelector(".swym-registry-gifter-banner-user").textContent = swymRegistry_initials;
          document.querySelector(".swym-registry-gifter-banner-title").textContent = swymgr.registryDetails.registryName;
          document.querySelector(".swym-registry-gifter-banner-message").textContent = swymgr.registryDetails.description;
          if(swymgr.registryDetails.description.trim().length===0){
            document.querySelector(".swym-registry-gifter-banner-message").style.display='none';
          }

          // Add Cart Badges if embed enabled
          if(isShopifyCartEnabled) {
            swymRegistryAddCartBadge();
          }
        }
      }).catch(error => {
          _LTracker.push({
              'ErrorLocation': 'Get registry details for gifter API call - Gifter Template',
              'ErrorInfo': {
                "error": {
                  "msg": error.message,
                  "Reference_stack": error.stack
                }
              },
              'MerchantInfo': {
                'pid': swymRegistry_authData.swym_registry.pid,
                'shop': Shopify.shop,
                'themeId': Shopify.theme.id,
                'themeName': Shopify.theme.name
              },
              'CustomerInfo': {
                'registryId': registryId,
                'customerId': swymRegistry_authData.swym_registry.customerId,
                'customerEmail': swymRegistryCustomer.email,
                'userAgent': navigator.userAgent
              }
            }); 
      });
  }
}
swymRegistryAddOnloadEvent(swymRegistryOnloadHandler);
