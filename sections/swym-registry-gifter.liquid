<script>
const swymRegistry_lsName = "swymKeys";
const swym_currency = "{{ cart.currency.symbol }}"
function swymRegistryInventoryController(productEpi){
  const swymRegistryQuantity = document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-${productEpi}`).value;
  let disableCheckoutOnInventoryLapse = false;
  {%- if shop.metafields.swym_registry.disableCheckoutOnInventoryLapse != blank -%}
     disableCheckoutOnInventoryLapse = {{ shop.metafields.swym_registry.disableCheckoutOnInventoryLapse }}
  {%- endif -%}
  if(disableCheckoutOnInventoryLapse){
    let swymRegistryObj = swymgr.registryDetails.associatedListedProducts.find(swymObj => swymObj.epi == productEpi);
    if(swymRegistryQuantity > swymRegistryObj.inventoryQuantity){
      document.querySelector(`.swym-registry-registrant-update-btn-${productEpi}`).style.opacity = "0.4"
      document.querySelector(`.swym-registry-registrant-update-btn-${productEpi}`).disabled=true;
    }else{
      document.querySelector(`.swym-registry-registrant-update-btn-${productEpi}`).style.opacity = "1"
      document.querySelector(`.swym-registry-registrant-update-btn-${productEpi}`).disabled=false;
    }
  }
}
function swymRegistryChangeQtyOnClick(productEpi,swymRegistryChangeQtySpan,action = 'reduce'){
  document.querySelector(`#product-${productEpi}-btn`).childNodes['1'].textContent = "{{ settings.swym_registry_gifter_grid_addtobag_btn }}";
  if(action=='reduce'){
    swymRegistryChangeQty(swymRegistryChangeQtySpan);
  }
  else{
    swymRegistryChangeQty(swymRegistryChangeQtySpan, 'increase');
  }
  swymRegistryInventoryController(productEpi)
}
  

async function swymRegistryChangeQtyOnClickCartPage(productEpi,swymRegistryChangeQtySpan,action = 'reduce'){
  let disableCheckoutOnInventoryLapse = false;
  {%- if shop.metafields.swym_registry.disableCheckoutOnInventoryLapse != blank -%}
     disableCheckoutOnInventoryLapse = {{ shop.metafields.swym_registry.disableCheckoutOnInventoryLapse }}
  {%- endif -%}
  document.querySelector(`#product-${productEpi}-btn`).childNodes['1'].textContent = "{{ settings.swym_registry_gifter_grid_addtobag_btn }}";
  const swymRegistryQuantity = document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-gifter-cart-${productEpi}`).value;
  if(action=='reduce' && swymRegistryQuantity > 1){
    swymRegistryChangeQty(swymRegistryChangeQtySpan);
  }
  else{
    if(disableCheckoutOnInventoryLapse){
      let swymRegistryObj = swymgr.registryDetails.associatedListedProducts.find(swymObj => swymObj.epi == productEpi);
      if(parseInt(swymRegistryQuantity) + 1 > swymRegistryObj.inventoryQuantity){
        return false; 
      }else{
        swymRegistryChangeQty(swymRegistryChangeQtySpan, 'increase');
      }
    }else{
      swymRegistryChangeQty(swymRegistryChangeQtySpan, 'increase');
    }
  }
  swymgr.checkoutProducts[productEpi].quantity = document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-gifter-cart-${productEpi}`).value;
  if(swymRegistryQuantity >= 1){
    await showSwymRegistryGifterModal("qty");
  }
}
async function swymRegistryRemoveListItem(productEpi){
  document.querySelector(`#product-${productEpi}-btn`).childNodes['1'].textContent = "{{ settings.swym_registry_gifter_grid_addtobag_btn }}";
  delete swymgr.checkoutProducts[productEpi];
  await showSwymRegistryGifterModal("remove");
  const swymRegistryCartProduct = Object.keys(swymgr.checkoutProducts).length;
  document.getElementById("swym-registry-gifters-cart-count").innerHTML = `&nbsp;(${swymRegistryCartProduct})`;    
}
async function swymRegistryHandleQtyChange(productEpi, mode=""){
  if(mode == 'gifter'){
    swymRegistryInventoryController(productEpi);
  }else if(mode == 'cart'){
    const swymRegistryQuantity = document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-gifter-cart-${productEpi}`).value;
    let disableCheckoutOnInventoryLapse = false;
    {%- if shop.metafields.swym_registry.disableCheckoutOnInventoryLapse != blank -%}
       disableCheckoutOnInventoryLapse = {{ shop.metafields.swym_registry.disableCheckoutOnInventoryLapse }}
    {%- endif -%}
    if(disableCheckoutOnInventoryLapse){
      let swymRegistryObj = swymgr.registryDetails.associatedListedProducts.find(swymObj => swymObj.epi == productEpi);
      if(swymRegistryQuantity > swymRegistryObj.inventoryQuantity){
        document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-gifter-cart-${productEpi}`).value = swymRegistryObj.inventoryQuantity;
        swymgr.checkoutProducts[productEpi].quantity = swymRegistryObj.inventoryQuantity;
      }else{
        swymgr.checkoutProducts[productEpi].quantity = document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-gifter-cart-${productEpi}`).value;
      }
    }else{
      swymgr.checkoutProducts[productEpi].quantity = document.querySelector(`.swym-registry-gifter-grid-item-bottom-qty-gifter-cart-${productEpi}`).value;
    }
    await showSwymRegistryGifterModal("qty");
  }
  
}
  
function swymRegistryIsAvailableInOnlineStore(product){
  let inPOS=false;
  let inOnlineStore=false;
  product.salesChannels.map(channel => {
      if(channel.name === "Online Store"){
        inOnlineStore=true;
      }
      else if(channel.name === "Point of Sale"){
        inPOS=true;
      }
  })
  return !(inPOS && !inOnlineStore);
}

function swymRegistryAddProduct(product, registry) {
  try{
    var swymRegistryGiftProduct = document.createElement('li');
    swymRegistryGiftProduct.classList.add("swym-registry-gifter-grid-item");
    const isAvailable = swymRegistryIsAvailableInOnlineStore(product);
    swymRegistryGiftProduct.innerHTML = `
          <a href="${product.du}" class="{{ settings.swym_registry_custom_link_class }}">
            <figure id="swym-registry-product-${product.epi}">
              <img src="${product.iu}" alt="swym-registry-product" class="swym-registry-product-image-${product.epi}">
            </figure>
          </a>
          <div class="swym-registry-gifter-grid-item-detail swym-registry-gifter-grid-item-container-${product.epi}">
            <h2 class="swym-registry-font-regular">${product.dt}</h2>
            <ul class="swym-registry-gifter-grid-item-center">
              <li><span class="swym-registry-gifter-grid-item-price swym-registry-font-large">${swym_currency}${product.pr}</span></li>
              <li class="swym-registry-gifter-grid-item-askqty-container swym-registry-font-very-small">
                <span class="swym-registry-gifter-grid-item-want">{{ settings.swym_registry_want_label }}</span>
                <span class="swym-registry-gifter-grid-item-want-value"> ${product.askQuantity}</span>
              </li>
              <li class="swym-registry-gifter-grid-item-boughtqty-container swym-registry-font-very-small">
                <span class="swym-registry-gifter-grid-item-purchased">{{ settings.swym_registry_purchase_label }}</span>
                <span class="swym-registry-gifter-grid-item-want-value"> ${product.boughtQuantity}</span>
              </li>
              <li class="swym-registry-gifter-grid-item-cartqty-container">
                <span class="swym-registry-font-small">
                  <strong>
                    <span class="swym-registry-cart-item-quantity">0</span> {{ settings.swym_registry_gifter_grid_addedtobag_btn }}
                  </strong>
                </span>
              </li>
            </ul>
            <ul class="swym-registry-gifter-grid-item-bottom">
              ${
                 isAvailable ? 
                `<li>
                <span class="swym-registry-gifter-grid-item-bottom-reduce" onclick="swymRegistryChangeQtyOnClick(${product.epi},this)">-</span>
                <input type="number" value="1" class="swym-registry-gifter-grid-item-bottom-qty swym-registry-gifter-grid-item-bottom-qty-${product.epi}" pattern="[0-9]*" aria-label="quantity" onChange="swymRegistryHandleQtyChange(${product.epi}, 'gifter')">
                <span class="swym-registry-gifter-grid-item-bottom-increase" onclick="swymRegistryChangeQtyOnClick(${product.epi},this, 'increase')">+</span>
              </li>
              <li class="swym-registry-gifter-update-btn-container" id="product-${product.epi}-btn">
                <button class="{{ settings.swym_registry_custom_btn_class }} swym-registry-font-btn swym-registry-registrant-update-btn-${product.epi}" onclick="swymUpdateCheckoutProducts(this, ${product.epi})" class="swym-registry-registrant-update-btn">{{ settings.swym_registry_gifter_grid_addtobag_btn }}</button>
              </li>` 
              : 
              `<div class="swym-registry-gifter-chip swym-registry-font-regular">
                <span>{{ settings.swym_registry_gifter_grid_chip }}</span>
              </div>`
              }
            </ul>
            <input type="hidden" value="${product.epi}" class="swym-registry-gifters-product-meta-epi">
            <input type="hidden" value="${product.empi}" class="swym-registry-gifters-product-meta-empi">
            <input type="hidden" value="${product.du}" class="swym-registry-gifters-product-meta-du">
            <input type="hidden" value="${product.inventoryPolicy}" class="swym-registry-gifters-product-meta-inventoryPolicy">
            <input type="hidden" value="${product.inventoryQuantity}" class="swym-registry-gifters-product-meta-inventoryQuantity">
          </div>`;
      
      var swymRegistryProductGrid = document.querySelector(".swym-registry-gifter-grid-items");
      swymRegistryProductGrid.appendChild(swymRegistryGiftProduct);
      
      if(isAvailable && swymRegistryGiftProduct.querySelector(".swym-registry-gifter-grid-item-bottom-qty")){
       swymRegistryGiftProduct.querySelector(".swym-registry-gifter-grid-item-bottom-qty").addEventListener("input", ( event ) => {
          let swymRegsitryTargetValue = event.target.value;
          if(swymRegsitryTargetValue<0){
            event.target.value = 0;
          }
        });
      }
      if(product && product.inventoryPolicy == 'DENY' && product.inventoryQuantity != undefined && product.inventoryQuantity != null &&  product.inventoryQuantity <= 0 && product.inventoryTracked === true){
        let swymRegistryImage = document.getElementById(`swym-registry-product-${product.epi}`)
        let swymRegistryImageTag = document.createElement('span');
        swymRegistryImageTag.classList.add('swym-registry-product-banner', 'swym-registry-font-small');
        swymRegistryImageTag.innerHTML=`{{ settings.swym_registry_sold_out_label }}`;
        swymRegistryImage.appendChild(swymRegistryImageTag);
      } else if(product.boughtQuantity >= product.askQuantity){
        let swymRegistryImage = document.getElementById(`swym-registry-product-${product.epi}`)
        let swymRegistryImageTag = document.createElement('span');
        swymRegistryImageTag.classList.add('swym-registry-product-banner', 'swym-registry-font-small');
        swymRegistryImageTag.innerHTML=`{{ settings.swym_registry_purchased_label }}`;
        swymRegistryImage.appendChild(swymRegistryImageTag)
      }
    if(isAvailable) swymRegistryInventoryController(product.epi);
  } catch(error) {
      console.log("Error in showing registry pages", error);
  }    
}
function swymUpdateCheckoutProducts(button, epi){
    try {
      _LTracker.push({
        'eventInfo': 'Add to bag button clicked - Gifter',
        'tag': 'event'
      });
      Shopify.analytics.publish("swym_registry_gifter_view",{"eventAction": "Add to bag button clicked","customerInfo":{
        "name"  : "{{ customer.name }}",
        "email" : "{{ customer.email }}",
        "id"    : "{{ customer.id }}"
      }});
      if(!swymgr.checkoutProducts)
      {
        swymgr.checkoutProducts = {};
      }
      button.classList.remove('swym-gr-gifters-btn-secondary', 'swym-gr-font-small');
      button.classList.add('swym-gr-gifters-btn-inverse-secondary', 'swym-gr-font-small-light');
      if(document.querySelector(`.swym-registry-gifter-grid-item-container-${epi} .swym-registry-gifter-grid-item-bottom-qty`).value==0){
        delete swymgr.checkoutProducts[`${epi}`];
      }
      else{
        let swymRegistryProductDetails = {
          image: document.querySelector(`.swym-registry-product-image-${epi}`).src,
          description: document.querySelector(`.swym-registry-gifter-grid-item-container-${epi} .swym-registry-font-regular`).innerHTML,
