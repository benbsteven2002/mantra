        "swym_registry_home_how_to_step_2_number": "{{ settings.swym_registry_home_how_to_step_2_number }}",
        "swym_registry_home_how_to_step_3_number": "{{ settings.swym_registry_home_how_to_step_3_number }}",
        "swym_registry_home_how_to_step_1_text": "{{ settings.swym_registry_home_how_to_step_1_text }}",
        "swym_registry_home_how_to_step_2_text": "{{ settings.swym_registry_home_how_to_step_2_text }}",
        "swym_registry_home_how_to_step_3_text": "{{ settings.swym_registry_home_how_to_step_3_text }}",
        "swym_registry_signup_login_text": "{{ settings.swym_registry_signup_login_text }}",
        "swym_registry_login_account_create_text": "{{ settings.swym_registry_login_account_create_text }}",
        "swym_registry_login_password_label": "{{ settings.swym_registry_login_password_label }}",
        "swym_registry_login_email_label": "{{ settings.swym_registry_login_email_label }}",
        "swym_registry_login_btn_text": "{{ settings.swym_registry_login_btn_text }}",
        "swym_registry_signup_btn_text": "{{ settings.swym_registry_signup_btn_text }}",
        "swym_registry_signup_password_label": "{{ settings.swym_registry_signup_password_label }}",
        "swym_registry_signup_email_label": "{{ settings.swym_registry_signup_email_label }}",
        "swym_registry_signup_lname_label": "{{ settings.swym_registry_signup_lname_label }}",
        "swym_registry_signup_fname_label": "{{ settings.swym_registry_signup_fname_label }}",
        "swym_registry_login_form_title": "{{ settings.swym_registry_login_form_title }}",
        "swym_registry_signup_form_title": "{{ settings.swym_registry_signup_form_title }}"
      }
    });
    function swymRegistryStylesApi(swymRegistry_styles_payload) {
      swymRegistry_uploadConfig(swymRegistry_styles_payload);
    }
    async function swymRegistry_uploadConfig(payload) {
      const swymRegistry_appProxyUrl = window.location.origin + `/a/swym-registry-proxy/storefront-config`;
      fetch(`${swymRegistry_appProxyUrl}`, {
        headers: {
          accept: "application/json",
          "Content-Type": "application/json"
        },
        body: payload,
        method: "POST"
      })
        .then((response) => response.json())
        .then((data) => {
          return data;
        })
        .catch((error) => {
          _LTracker.push({
            'ErrorLocation': 'Upload Config API call',
            'ErrorInfo': {
              "error": {
                "msg": error.message,
                "Reference_stack": error.stack
              }
            },
            'MerchantInfo': {
              'shop': Shopify.shop,
              'themeId': Shopify.theme.id,
              'themeName': Shopify.theme.name,
              'userAgent': navigator.userAgent
            }
          });
          console.log('Error in uploading configs', error, payload);
          return error;
        });
    }
    function swymRegistryStringsApi(swymRegistry_strings_payload) {
      swymRegistry_uploadConfig(swymRegistry_strings_payload);
    }
    swymRegistryStylesApi(swymRegistryStylesPayload);
    swymRegistryStringsApi(swymRegistryStringsPayload);
  }
</script>
